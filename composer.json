{
    "name": "in10/multilanguage",
    "description": "Simple, opinionated multilanguage package for Laravel",
    "type": "library",
    "require": {
        "php": "^7.1 || ^7.2 || ^7.3 || ^7.4 || ^8.1",
        "laravel/framework": ">8.0"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Jakob Buis",
            "email": "jakob@jakobbuis.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "IN10\\Multilanguage\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "IN10\\Multilanguage\\ServiceProvider"
            ]
        }
    }
}
